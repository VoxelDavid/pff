&#9;local Update = Scroll.Update
&#9;
&#9;local Class = {
&#9;&#9;List = List;
&#9;&#9;GUI = ScrollingListFrame;
&#9;&#9;Scroll = Scroll;
&#9;&#9;Update = Update;
&#9;&#9;EntryStylist = EntryStylist;
&#9;}
