&#9;&#9;end;
&#9;}

&#9;function Class.Destroy()
&#9;&#9;for k in pairs(Class) do
&#9;&#9;&#9;Class[k] = nil
&#9;&#9;end
&#9;&#9;SpriteFrame:Destroy()
&#9;end

&#9;return Class,SpriteFrame
