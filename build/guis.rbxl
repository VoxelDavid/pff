&#9;&#9;&#9;&#9;&#9;nodeX[i] = posX
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;if nodeX[i+1] &gt; modX then
&#9;&#9;&#9;&#9;&#9;nodeX[i+1] = modX
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;fillLine(nodeX[i]-posX,nodeX[i+1]-posX,pixelY-posY)
&#9;&#9;&#9;end
&#9;&#9;&#9;i = i + 2
&#9;&#9;end
&#9;end
