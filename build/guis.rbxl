&#9;&#9;-- update current entries
&#9;&#9;for i = 1,visible_space do
&#9;&#9;&#9;local item = List[i + Scroll.ScrollIndex]
&#9;&#9;&#9;if item then
&#9;&#9;&#9;&#9;local entry = EntryFrames[i]
&#9;&#9;&#9;&#9;if not entry then
&#9;&#9;&#9;&#9;&#9;entry = EntryTemplate:Clone()
